<div class="panel">
    <div class="panel-body">
        <h3 class="title-hero">
            New attraction
        </h3>
        <div class="example-box-wrapper">
            <form class="form-horizontal bordered-row" ng-submit="submit()">
                <span class="errors">
                    <div ng-repeat="err in errors" class="alert alert-danger">
                        {{err}}
                    </div>
                </span>
                <div class="form-group">
                    <label class="col-sm-3 control-label">Name</label>
                    <div class="col-sm-6">
                        <input type="text" class="form-control" id="" ng-model="form.name" name="name">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label">Open Time</label>
                    <div class="col-sm-6">
                        <input type="text" class="form-control input-mask" id="" ng-model="form.openTime" name="openTime" placeholder="hh:mm:ss" data-inputmask="&apos;mask&apos;:&apos;99:99:99&apos;">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label">Close Time</label>
                    <div class="col-sm-6">
                        <input type="text" class="form-control input-mask" id="" ng-model="form.closeTime" name="closeTime" placeholder="hh:mm:ss" data-inputmask="&apos;mask&apos;:&apos;99:99:99&apos;">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label">Price</label>
                    <div class="col-sm-6">
                        <input type="number" class="form-control" id="" ng-model="form.price" name="price" step="0.01" min="0.00">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label">Location</label>
                    <div class="col-sm-6">
                        <select class="custom-select" ng-model="form.location" name="location">
                            <option>Singapore</option>
                        </select>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label">Description</label>
                    <div class="col-sm-6">
                        <textarea id="" class="form-control" ng-model="form.description" name="description"></textarea>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label">Miniature</label>
                    <div class="col-sm-6">
                        <div class="fileinput fileinput-new" data-provides="fileinput">
                            <span class="btn btn-primary btn-file">
                                <span class="fileinput-new">Select file</span>
                                <span class="fileinput-exists">Change</span>
                                <input type="file" ng-model="form.img" name="img">
                            </span>
                            <span class="fileinput-filename"></span>
                            <a href="#" class="close fileinput-exists" data-dismiss="fileinput" style="float: none">&times;</a>
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label">Facebook</label>
                    <div class="col-sm-6">
                        <input type="text" class="form-control" id="" ng-model="form.facebook" name="facebook">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label">Instagram</label>
                    <div class="col-sm-6">
                        <input type="text" class="form-control" id="" ng-model="form.instagram" name="instagram">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label"></label>
                    <div class="col-sm-6">
                        <input type="submit" class="btn btn-primary" id="" value="Create attraction">
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>